{
    "executionRoleArn": "arn:aws:iam::229706420082:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "sample-apiserver",
            "image": "<IMAGE_NAME>",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 8082,
                    "protocol": "tcp",
                    "containerPort": 8082
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "ecs-demo-spring"
}
