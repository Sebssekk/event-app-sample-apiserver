{
    "executionRoleArn": "arn:aws:iam::229706420082:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "sample-apiserver",
            "image": "<IMAGE_NAME>",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 8082,
                    "protocol": "tcp",
                    "containerPort": 8082
                }
            ],
            "healthCheck": {
              "retries": 3,
              "command": [
                  "CMD-SHELL",
                  "curl http://127.0.0.1:8082/ || exit 1"
              ],
              "timeout": 5,
              "interval": 30,
              "startPeriod": 30
          },
          "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "awslogs-apiserver",
                    "awslogs-region": "eu-west-3",
                    "awslogs-stream-prefix": "awslogs-demo-ecs"
                }
            }
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "ecs-demo-spring"
}
